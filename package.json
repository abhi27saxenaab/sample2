{
  "name": "sequelize-sqlite-project",
  "version": "1.0.0",
  "description": "Node.js + SQLite + Sequelize + Migrations sample project",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
     



    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "migrate:status": "npx sequelize-cli db:migrate:status",
    "migrate:create": "npx sequelize-cli migration:generate --name",
    "db:reset": "npm run migrate:undo:all && npm run migrate && npm run seed",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo:all",
    "seed:create": "npx sequelize-cli seed:generate --name"

  },
  "dependencies": {
    "sequelize": "^6.35.1",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "sequelize-cli": "^6.6.2",
    "nodemon": "^3.0.2"
  }
}